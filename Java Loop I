import java.util.Scanner;
public class table
{
    public static void main(String []args)
    {
        
        int n,r;
        Scanner scan =new Scanner(System.in);
        n=scan.nextInt();
        for(int i=1;i<11;i++)
        {
            r=n*i;
            System.out.println(n+ " x "+ i+ " = "+r);
            
        }
    }
}
